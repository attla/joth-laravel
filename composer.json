{
    "name": "attla/joth-laravel",
    "description": "Encrypt and decrypt requests and responses data on laravel.",
    "keywords": [
        "attla",
        "laravel",
        "joth",
        "joth-laravel",
        "request",
        "response",
        "encrypt",
        "decrypt"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolau",
            "email": "nicolau@zunq.com"
        }
    ],
    "homepage": "https://github.com/attla/joth-laravel",
    "require": {
        "php": ">=7.2.5",
        "attla/support": "dev-main",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/http": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Attla\\Joth\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Attla\\Joth\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
