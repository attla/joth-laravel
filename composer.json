{
    "name": "octha/joth-laravel",
    "description": "Encrypt and decrypt requests and responses data on laravel.",
    "keywords": [
        "attla",
        "laravel",
        "joth",
        "joth-laravel",
        "request",
        "response",
        "encrypt",
        "decrypt"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Nicolau",
            "email": "lucas@octha.com"
        }
    ],
    "homepage": "https://github.com/octhahq/joth-laravel",
    "require": {
        "php": ">=5.6"
    },
    "autoload": {
        "psr-4": {
            "Octha\\Joth\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Octha\\Joth\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
